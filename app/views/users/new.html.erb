<h5>register</h5>

<%= render 'shared/errors', obj: @user %>

<%= form_for @user do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>
</br>
  <%= f.label :password %>
  <%= f.password_field :password %>
</br>
<%= f.label :time_zone %>
<%= f.time_zone_select :time_zone, ActiveSupport::TimeZone.us_zones %>
</br>
  <%= f.submit 'register', class: 'btn btn-primary' %>
<% end %>
